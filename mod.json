{
	"geode": "4.3.1",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "ninsam.to-do_list",
	"name": "To-Do List",
	"version": "v1.1.4",
	"developer": "NinSam",
	"description": "Adds a To-Do List in the GD editor",
	"dependencies":{

		"geode.node-ids": {
			"version": "1.20.1",
            "importance": "required"
		}
	},
	"tags": [
		"offline",
		"editor",
		"utility"
	],
	"links": {
		"source": "https://github.com/NinSam/To-Do-List"
	},
	"resources": {
		"sprites":["resources/*.png"]
    },
	"settings":{

		"title-1": {
            "type": "title",
            "name": "Theme"
        },
		"theme-presets": {
            "type": "string",
            "name": "Theme Presets",
            "default": "Default",
			"one-of": ["Default", "Blue", "Green", "Purple", "Gray"]
        },
		"custom-theme-enable": {
			"type": "bool",
            "name": "Custom Theme",
            "default": false
        },
		"custom-theme": {
			"type": "rgb",
            "name": "Theme Color",
			"default": "#5B5C5C",
			"enable-if": "custom-theme-enable"
        },
		"title-2": {
            "type": "title",
            "name": "Other"
        },
		"button-color": {
            "type": "string",
            "name": "Button Color",
            "default": "Default",
			"one-of": ["Default", "Red", "Orange", "Yellow", "Green", "Blue", "Purple", "Pink"]
        }
	
	}

}
